{
  "name": "server",
  "version": "1.0.0",
  "description": "Backend Express server for Clogged",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon -L src/index.ts",
    "dev:worker": "nodemon -L src/worker.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate:create": "node-pg-migrate -m migrations create",
    "migrate:up": "node-pg-migrate -m migrations up",
    "migrate:down": "node-pg-migrate -m migrations down"
  },
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.9.0",
    "bullmq": "^5.51.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "ioredis": "^5.6.1",
    "node-pg-migrate": "^7.9.1",
    "pg": "^8.15.6",
    "pino": "^8.0.0",
    "pino-http": "^10.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.2",
    "@types/pg": "^8.11.14",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
